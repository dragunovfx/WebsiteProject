{
  "version": 2,
  "buildCommand": "cd ProjectWebsite && rm -rf node_modules && npm install --legacy-peer-deps && GENERATE_SOURCEMAP=false NODE_ENV=production npm run build",
  "outputDirectory": "ProjectWebsite/build",
  "installCommand": "cd ProjectWebsite && npm install --legacy-peer-deps",
  "framework": "create-react-app",
  "env": {
    "NODE_ENV": "production",
    "GENERATE_SOURCEMAP": "false"
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/logo192.png",
      "dest": "/logo192.png"
    },
    {
      "src": "/logo512.png",
      "dest": "/logo512.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "github": {
    "enabled": true,
    "silent": true
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
} 